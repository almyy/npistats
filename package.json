    "babel-runtime": "^6.26.0",
    "cheerio": "^1.0.0-rc.2",
    "classnames": "^2.2.5",
    "connect": "^3.6.5",
    "express": "^4.16.2",
    "express-graphql": "^0.6.11",
    "flow": "^0.2.3",
    "flow-bin": "^0.63.1",
    "graphql": "^0.12.3",
    "graphql-tools": "^2.18.0",
    "moment": "^2.20.1",
    "mongodb": "^3.0.1",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "password-generator": "^2.2.0",
    "path": "^0.12.7",
    "react": "^16.2.0",
    "react-apollo": "^2.0.4",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "uuid": "^3.1.0"
  },
  "scripts": {
    "frontend": "run-p start-js watch-css",
    "start-js": "react-scripts start",
    "build-fe": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer build/static/js/main.*",
    "build-be": "rimraf dist/backend && babel ./backend --out-dir dist/backend --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "backend": "npm run build-be && node dist/backend/index.js",
    "build-scrape": "rimraf dist/scrape && babel ./scrapi --out-dir dist/scrape --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "scrape": "npm run build-scrape && node dist/scrape/index.js",
    "start": "run-p backend start-js",
    "flow": "flow",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive"
  },
  "proxy": "http://localhost:5000",
  "devDependencies": {
    "source-map-explorer": "^1.5.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "rimraf": "^2.6.2"
  }
